
                    <section class="container-fluid text-center mt-5 font-size-22 font-weight-bold">
                        <% if(currentPage!=1 && previousPage!= 1){ %> 
                            <a href="?page=1">1</a>
                        <% } %>
                        <% if(hasPreviousPage){ %> <a href="?page=<%=previousPage%>">
                                        <%=previousPage%>
                                    </a>
                        <% } %>
                        <a href="?page=<%=currentPage%>" class="text-success">
                                    <%=currentPage%>
                        </a>
                        <% if(hasNextPage){ %> <a href="?page=<%=nextPage%>">
                                        <%=nextPage%>
                                    </a>
                        <% } %>
                        <% if(lastPage!=currentPage && nextPage!=lastPage){ %> <a href="?page=<%=lastPage%>" >
                                                <%=lastPage%>
                                            </a>
                        <% } %>
                    </section>